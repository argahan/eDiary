@model IEnumerable<E_GUNLUK.Models.FriendsList>


@{ 
    Layout = null;
}

@if (Model.Count() == 0)
{
    return;
}
else { 
<h4>Friends count: [@Model.Count()]</h4>
<ul>
    @foreach (var item in Model)
    {
        

        <li><a href="/Profiles/Details/@item.friend_user.Id">@item.friend_user.FirstName @item.friend_user.LastName</a>| <span>@Html.Action("AddFriend", "Profiles", new { item.friend_user.Id})</span>
</li>
    }
</ul>
}